<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!--导航栏-->
    <declare-styleable name="NavTitleBar">
        <attr name="leftTextShow" format="boolean" />
        <attr name="leftText" format="string" />
        <attr name="leftTextSize" format="dimension" />
        <attr name="rightTextShow" format="boolean" />
        <attr name="rightText" format="string" />
        <attr name="rightTextSize" format="dimension" />
        <attr name="rightTextShowWithIcon" format="boolean" />
        <attr name="rightTextWithIcon" format="string" />
        <attr name="rightTextSizeWithIcon" format="dimension" />
        <attr name="backGroundColor" format="color" />
        <attr name="barStatusMargin" format="boolean" />
        <attr name="backGroundAlpha" format="float" />
        <attr name="rightIcon" format="reference" />
        <attr name="rightIconShow" format="boolean" />
        <attr name="rightIconSize" format="dimension" />
        <attr name="middleText" format="string" />
        <attr name="middleTextShow" format="boolean" />
        <attr name="showBackIcon" format="boolean" />
        <attr name="backIcon" format="reference" />
    </declare-styleable>

    <declare-styleable name="SwipeMenuLayout">
        <attr name="swipeEnable" format="boolean"/>
        <attr name="ios" format="boolean"/>
        <attr name="leftSwipe" format="boolean"/>
    </declare-styleable>

</resources>